{
    "name": "lumphp/laravel-testing",
    "description": "The Illuminate Testing package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/lumphp/laravel-testing/issues",
        "source": "https://github.com/lumphp/laravel-testing"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "lumphp/laravel-collections": "^8.0",
        "lumphp/laravel-contracts": "^8.0",
        "lumphp/laravel-macroable": "^8.0",
        "lumphp/laravel-support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Testing\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        }
    },
    "suggest": {
        "brianium/paratest": "Required to run tests in parallel (^6.0).",
        "lumphp/laravel-console": "Required to assert console commands (^8.0).",
        "lumphp/laravel-database": "Required to assert databases (^8.0).",
        "lumphp/laravel-http": "Required to assert responses (^8.0).",
        "mockery/mockery": "Required to use mocking (^1.4.2).",
        "phpunit/phpunit": "Required to use assertions and run tests (^8.5.8|^9.3.3)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
